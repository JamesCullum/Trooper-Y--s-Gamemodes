	    	    new Float:x1,Float:y1,Float:z1;
	    	    GetPlayerPos(i,x1,y1,z1);
				if(PlayerToPoint(1.5,playerid,x1,y1,z1))
				{
                    GetPlayerName(i, name, sizeof(name));
				    format(string, sizeof(string), "%s got zombified.", name );
				    SendClientMessageToAll(COLOR_GREY, string);
				    
					SetPlayerColor(i,COLOR_RED);
					SetPlayerHealth(i,50);
					SendClientMessage(i,COLOR_RED,"You turned into a zombie ! Try to infect all other players !");
					SetPlayerSkin(i,162);
					ResetPlayerWeapons(i);
					score[i] = GetPlayerScore(i);
					SetPlayerScore(playerid,score[i] + 1);
					zombie[i] = 1;
     				new humans = 0;
					for(new r; r<MAX_PLAYERS; r++)
					{
					    if(IsPlayerConnected(r) && zombie[r] == 0)
					    {
							humans = humans+1;
					    }
					}
					format(string, sizeof(string), "%i Humans left", humans );
				    SendClientMessageToAll(COLOR_GREY, string);
					if(humans == 0)
					{
					   	for(new f; f<MAX_PLAYERS; f++)
						{
							if(IsPlayerConnected(f))
							{
								GameTextForPlayer(f,"~w~Zombies won",1000,5);
								newround[i] = 1;
							}
						}
						SetTimer("newmap", 5000, 0);
					}
				}
			}
		}
    }
    return 1;
}

