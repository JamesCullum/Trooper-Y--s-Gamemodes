stock PlayerToPoint(Float:radi, playerid, Float:x, Float:y, Float:z)
{
        new Float:oldposx, Float:oldposy, Float:oldposz;
        new Float:tempposx, Float:tempposy, Float:tempposz;
        GetPlayerPos(playerid, oldposx, oldposy, oldposz);
        tempposx = (oldposx -x);
        tempposy = (oldposy -y);
        tempposz = (oldposz -z);
        if (((tempposx < radi) && (tempposx > -radi)) && ((tempposy < radi) && (tempposy > -radi)) && ((tempposz < radi) && (tempposz > -radi)))
        {
                return 1;
        }
        return 0;
}


public OnPlayerText(playerid, text[])
{
	new myvar[128];
    format(myvar,sizeof(myvar),"%s",text);

	if(zombie[playerid] == 1)
	{
	    for(new i; i<MAX_PLAYERS; i++)
	    {
	        if(IsPlayerConnected(i) && zombie[i] == 1)
	        {
	            SendPlayerMessageToPlayer(i,playerid,myvar);
			}
		}
 	}
 	else if(zombie[playerid] == 0)
	{
	    for(new i; i<MAX_PLAYERS; i++)
	    {
	        if(IsPlayerConnected(i) && zombie[i] == 0)
	        {
	            SendPlayerMessageToPlayer(i,playerid,myvar);
			}
		}
 	}
    return 0;
}

public OnPlayerEnterCheckpoint(playerid)
{
	if(zombie[playerid] == 1)
	{
	    DisablePlayerCheckpoint(playerid);
	}
	else if(zombie[playerid] == 0 && map == 2)
	{
	    DisablePlayerCheckpoint(playerid);
		SetPlayerSpecialAction(playerid,2);
	    GameTextForPlayer(playerid,"Escape !!!",1,1);
	    SetTimerEx("ze_win",5000,0,"i",playerid);
	}
	else if(zombie[playerid] == 0 && map == 3)
	{
	    DisablePlayerCheckpoint(playerid);
	    GameTextForPlayer(playerid,"Enter the Hunter !",3,1);
 	}
}

public ze_win(playerid)
{
	if(zombie[playerid] == 0)
	{
        for(new i; i<MAX_PLAYERS; i++)
	    {
	        if(IsPlayerConnected(i))
	        {
	            GetPlayerName(playerid, name, sizeof(name));
				format(string, sizeof(string), "%s made the Humans win !", name );
			    SendClientMessageToAll(COLOR_GREEN, string);
			    break;
			}
		}
	    EndOfRound();
	}
	else
 	{
		RemovePlayerFromVehicle(playerid);
		SetPlayerPos(playerid,-1000,-1000,0);
		SetPlayerHealth(playerid,0);
		CreateExplosion(-1000,-1000,0,2,5);
		SendClientMessage(playerid,COLOR_GREY,"You got infected before escaping sucessfully !");
	}
}

