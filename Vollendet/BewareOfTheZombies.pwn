public OnPlayerDeath(playerid, killerid, reason)
{
	if(zombie[playerid] == 1 && zombie[killerid] == 0)
	{
	    GivePlayerMoney(killerid,250);
	}
	if(zombie[playerid] == 0 && zombie[killerid] == 1 && newround[playerid] == 0)
	{
	    SetPlayerColor(playerid,COLOR_RED);
	    SendClientMessage(playerid,COLOR_RED,"You turned into a zombie ! Try to infect all other players !");
	    SetPlayerSkin(playerid,162);
	    ResetPlayerWeapons(playerid);
	    zombie[playerid] = 1;
	    score[killerid] = GetPlayerScore(killerid);
		SetPlayerScore(killerid,score[killerid] + 1);
	}
	else if(zombie[playerid] == 0 && zombie[killerid] == 0)
	{
		SendClientMessage(killerid,COLOR_RED,"Do not Teamkill !");
		Kick(killerid);
 	}
 	if(zombie[playerid] == 1)
 	{
 	    SetPlayerColor(playerid,COLOR_RED);
 	    SetPlayerSkin(playerid,162);
	    ResetPlayerWeapons(playerid);
 	}
	new humans = 0;
	for(new i; i<MAX_PLAYERS; i++)
	{
	    if(IsPlayerConnected(i) && zombie[i] == 0)
	    {
			humans = 1;
	    }
	}
	if(humans == 0 && newround[playerid] == 0)
	{
     	for(new i; i<MAX_PLAYERS; i++)
		{
		    if(IsPlayerConnected(i))
		    {
    			GameTextForPlayer(i,"~w~Zombies won",1000,5);
		    }
		}
		SetTimer("newmap", 5000, 0);
		newround[playerid] == 1;
	}
	if(newround[playerid] == 0)
	{
		SetTimerEx("SetPlayerSpawn",790,0,"i",playerid);
	 	SpawnPlayer(playerid);
 	}
	return 1;
}


