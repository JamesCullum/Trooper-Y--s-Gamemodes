{
	if(strcmp(cmdtext,"/props",true)==0)
		{
		ShowMenuForPlayer(bau, playerid);
		TogglePlayerControllable(playerid, false);
		return 1;
	}
    if (strcmp("/afk", cmdtext, true, 9) == 0)
    {
        SpawnPlayer(playerid);
