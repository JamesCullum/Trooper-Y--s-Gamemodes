	    SetPlayerHealth(playerid,50);
 		SetPlayerColor(playerid,COLOR_RED);
	    SendClientMessage(playerid,COLOR_RED,"You turned into a zombie ! Try to infect all other players !");
	    SetPlayerSkin(playerid,162);
	    ResetPlayerWeapons(playerid);
	    zombie[playerid] = 1;
    	new humans = 0;
		for(new i; i<MAX_PLAYERS; i++)
		{
		    if(IsPlayerConnected(i) && zombie[i] == 0)
		    {
				humans = 1;
		    }
		}
		if(humans == 0 && newround[playerid] == 0)
		{
	     	for(new i; i<MAX_PLAYERS; i++)
			{
			    if(IsPlayerConnected(i))
			    {
	    			GameTextForPlayer(i,"~w~Zombies won",1000,5);
	    			newround[i] = 1;
			    }
			}
			SetTimer("newmap", 5000, 0);
			
		}
		return 1;
	}
	if (strcmp("/help", cmdtext, true, 5) == 0)
	{
	    SendClientMessage(playerid,COLOR_GREY,"/zombieme -> Zombify yourself");
	    SendClientMessage(playerid,COLOR_GREY,"***Maps*** In ZE (Zombie Escape) Maps you have to escape from Zombies");
	    SendClientMessage(playerid,COLOR_GREY,"***Maps*** In ZM (Zombie Murder) Maps you have to hide yourself for 10 Minutes !");
	    SendClientMessage(playerid,COLOR_GREY,"***Aim*** Aim of Zombies -> Infect all Humans");
	    return 1;
	}
	if (strcmp("/kill", cmdtext, true, 5) == 0)
	{
	    SetPlayerHealth(playerid,0);
	    return 1;
	}
	if (strcmp("/cod5bug", cmdtext, true, 8) == 0 && IsPlayerAdmin(playerid))
	{
	    zbug = 1;
	    return 1;
	}
	return 0;
}


